library(dplyr)
library(readxl)
load("scrape/data/reds.RData")
xl = read_excel("scrape/reds_manual/reds.xlsx", sheet = "2017",
range = "A1:B380", col_names = FALSE)
xl = cbind(1:380, xl)
names(xl) = c("Match", "c1", "c2")
xl$c1[which(is.na(xl$c1))] = 0
xl$c2[which(is.na(xl$c2))] = 0
cnt = reds %>%
filter(Season == 2017) %>%
count(Match)
fj = full_join(cnt, xl)
fj$n[is.na(fj$n)] = 0
fj = fj %>%
arrange(Match)
fjV = fj %>%
rowwise() %>%
mutate(c = c1-c2,
dif = n - c) %>%
filter(dif != 0)
View(fjV)
